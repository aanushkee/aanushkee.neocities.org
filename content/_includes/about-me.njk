---
noCache: "20241130"
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <title>{{ title }}{% if title !== "petrapixel" %} | petrapixel{% endif %}</title>
    {% if not noSEO %}
    <meta
      name="description"
      content="{{ description or 'Welcome to petrapixel! Here you can find many resources for Neocities, coding help, media recommendations and more in a cute Old Web aesthetic!' }}"
    />
    {% endif %}
    <script src="{{ nesting }}assets/js/main.js?nocache={{ noCache }}"></script>
    <link rel="stylesheet" href="{{ nesting }}assets/main.css?nocache={{ noCache }}" />
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type" />
    <meta content="utf-8" http-equiv="encoding" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="{{ nesting }}assets/img/favicon.ico" rel="icon" type="image/x-icon" />

    {% if not noSEO and not testingOnLocalhost %}
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-J64TL8KVTJ"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {
        dataLayer.push(arguments);
      }
      gtag("js", new Date());
      gtag("config", "G-J64TL8KVTJ");
    </script>
    {% endif %}
  </head>
  <body class="about-me-page load-theme-switcher">
    <noscript id="no-js-warning">Please enable JavaScript to view this website!</noscript>
    <a href="#main" id="skip-to-content-link">Skip to content</a>
    <a href="/sitemap.html" id="skip-to-content-link2">Go to Sitemap</a>
    <a href="/" class="about-me-page-back">‚Üê back</a>
    <div id="content">
      <aside>
        <nav>
          <ul aria-label="Navigation">
            <li class="{% if title == "about" %}active{% endif %}">
              <a href="/about-me/about">
                <img aria-hidden="true" src="../assets/img/layout/aboutpngs/bow.png" alt="" />
                <div><span>about</span></div>
              </a>
            </li>
            <li class="{% if title == "likes" %}active{% endif %}">
              <a href="/about-me/likes">
                <img aria-hidden="true" src="../assets/img/layout/aboutpngs/pc.png" alt="" />
                <div><span>likes</span></div>
              </a>
            </li>
            <li  class="{% if title == "favorites" %}active{% endif %}">
              <a href="/about-me/favorites">
                <img aria-hidden="true" src="../assets/img/layout/aboutpngs/tv.png" alt="" />
                <div><span>favorites</span></div>
              </a>
            </li>
            <li  class="{% if title == "cats" %}active{% endif %}">
              <a href="/about-me/cats">
                <img aria-hidden="true" src="../assets/img/layout/aboutpngs/paw.png" alt="" />
                <div><span>cats</span></div>
              </a>
            </li>
            <li class="{% if title == "media log" %}active{% endif %}">
              <a href="/about-me/media-log">
                <img aria-hidden="true" src="../assets/img/layout/aboutpngs/ds.png" alt="" />
                <div><span>media log</span></div>
              </a>
            </li>
            <li class="{% if title == "elsewhere" %}active{% endif %}">
              <a href="/about-me/elsewhere">
                <img aria-hidden="true" src="../assets/img/layout/aboutpngs/phone.png" alt="" />
                <div><span>elsewhere</span></div>
              </a>
            </li>
          </ul>
        </nav>
      </aside>
      <main id="main" class="custom-scrollbar">
        {{ content | safe }}
      </main>
    </div>
    {% if lightbox %}
    <script src="../assets/js/fslightbox.js"></script>
    {% endif %}
  </body>
</html>
